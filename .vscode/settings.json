{
  "swift.path": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin",
  "swift.swiftEnvironmentVariables": {
    "DEVELOPER_DIR": "/Applications/Xcode.app/Contents/Developer"
  },
  "files.associations": {
    "*.swift": "swift"
  },
  "editor.semanticHighlighting.enabled": true,
  "lldb-dap.executable-path": "/Applications/Xcode.app/Contents/Developer/usr/bin/lldb-dap",
  "lldb.executable": "/Applications/Xcode.app/Contents/Developer/usr/bin/lldb",
  "sweetpad.project.buildConfiguration": "Debug",
  "sweetpad.project.scheme": "DDLedBanner",
  "sweetpad.project.destination": "platform=iOS Simulator,name=iPhone 16 Pro",
  "sweetpad.project.sdk": "iphonesimulator",
  "sweetpad.project.xcodeproj": "./DDLedBanner/DDLedBanner.xcodeproj",
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off",
    "other": "off"
  },
  "swiftlint.enable": false,
  "swiftlint.run": "never"  
}
